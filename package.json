{
  "name": "illuminate",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Michael Hamilton <mike@miska.me>",
  "license": "Apache-2.0",
  "scripts": {
    "rebuild": "electron-rebuild",
    "dev": "parcel src/index.html & wait-on http://localhost:1234 && NODE_ENV=development electron .",
    "build": "parcel build src/index.html",
    "package": "pnpm run build && electron-builder --dir",
    "dist": "pnpm run build && electron-builder"
  },
  "build": {
    "appId": "illuminate",
    "productName": "Illuminate",
    "directories": {
      "buildResources": "resources",
      "output": "build"
    },
    "files": [
      "index.js",
      "server/**/*",
      "dist/**/*",
      "resources/**/*",
      "preload.js"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "electron-icon-maker": "^0.0.5",
    "electron-rebuild": "^3.2.9",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "dmxus": "git+ssh://git@github.com:michael-hamilton/dmxus.git",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "alias": {
    "process": false
  }
}
